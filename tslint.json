{
  "extends": [
    "tslint:latest",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false,
        "tabWidth": 2
      }
    ]
  }
}